    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dxc="using:DevExpress.WinUI.Core">

    <Style TargetType="dxc:TestControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dxc:TestControl">
                    <Border/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
